# Environment Configuration
NODE_ENV=development
PORT=5000
FRONTEND_URL=http://192.168.1.7:3001

# Database Configuration
MONGODB_URI=mongodb://localhost:27017/theater_canteen_db

# JWT Configuration
JWT_SECRET=yqpaynow-super-secret-jwt-key-development-only
JWT_EXPIRES_IN=24h
JWT_REFRESH_SECRET=yqpaynow-super-secret-refresh-key-development-only
JWT_REFRESH_EXPIRES_IN=7d

# Google Cloud Storage (Optional)
GCS_PROJECT_ID=
GCS_BUCKET_NAME=theater-canteen-uploads
GCS_KEY_FILE=
GCS_MOCK_MODE=false

# Upload Configuration
MAX_FILE_SIZE=50mb
UPLOAD_DIR=uploads

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=1000

# Logging
LOG_LEVEL=info

# Default Admin Credentials
DEFAULT_ADMIN_USERNAME=admin111
DEFAULT_ADMIN_PASSWORD=admin111